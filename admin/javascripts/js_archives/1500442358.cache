			}
		} else {
			working();

			$.ajax({
			  type: "POST",
			  url: "newsletterActions.php",
			  data: "newsletterAction=addCategory&name="+encodeURIComponent($("#rightPaneSubscriberCategories .addSubscriberCategory .focus").val()),
