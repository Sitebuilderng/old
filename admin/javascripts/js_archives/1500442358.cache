	$(document).unbind("keypress");

	assignSelectionKeys();

	$(document).keydown(function (e) {
		//alert(e.keyCode);
		if (e.keyCode==90) { // z
			if (metaDown&&!shiftDown) {
				undo();
			}
			if (metaDown&&shiftDown) {
				redo();
			}
			return false;
		}
		if (e.keyCode==78) { // n
			changeStyleOL();
		}
		if (e.keyCode==66) { // b
			changeStyleUL();
		}
		/*
		if (e.keyCode==88) { // x
			if (modDown) {
				cut();
				e.preventDefault();
			}
		}*/
