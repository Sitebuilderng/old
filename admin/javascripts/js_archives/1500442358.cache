			saveOrCancel();
			return false;
	});
	$(document).keypress(function(e){
		if (e.keyCode==13) { // enter
			e.preventDefault();
			return false;
		}

	});
	$(document).keyup(function(e){

		if (e.keyCode==13) { // enter
			e.preventDefault();
			return false;
		}
		if (e.keyCode==27) { // esc

			return false;
		}
	});
	$(document).keydown(function(e){
		if (e.keyCode==13) { // enter
			e.preventDefault();

			if (!shiftDown) {
				saveOrCancel();
			}
			return false;
		}
		if (e.keyCode==27) { // esc
			saveOrCancel();
			return false;
		}
	});
