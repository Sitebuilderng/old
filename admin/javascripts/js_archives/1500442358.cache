function menuScroll (obj,dontMove) {
	if (!obj.hasClass("menuScrollDone")) {
		if (typeof(dontMove)=="undefined") {
			if ($(".bpe_menu_bottom .bpe_menu_bottom",obj).length>0) { // has flyouts, use up/down buttons
				var height = obj.height();
