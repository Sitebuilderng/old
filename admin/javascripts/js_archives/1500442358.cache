				wHeight = wHeight - 68;
				if (occ>wHeight)
				{
					var sub = occ-wHeight;

					if (sub<ofTop)
					{
						sub = sub-45;
						// just need to move it up
						obj.css("top","-"+sub+"px").addClass("menuScrollDone");
					}
