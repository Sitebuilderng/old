		if (ignoreClickCatchup) {return false;}
		if (ismac) {
			if (e.ctrlKey) {
				return false;
			}			
		}
		if ($(e.target).hasClass("paneTools") || $(e.target).parents(".paneTools").length) {
			return false;
		}
		var el = e.target.tagName.toLowerCase();
