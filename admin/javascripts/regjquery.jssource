		var value = $(this).text();
		$(".listItemHighlight",$scope).data(variable,value);
		ajaxPath = ajaxPath+"&"+variable+"="+encodeURIComponent(value);
	});
	var path = ajaxPath.split("?");
	$.ajax({
		type: "POST",
		url: path[0],
