			if (Ids=="[") {
			Ids+='"'+$(this).data("id")+'"';
			} else {
			Ids+=',"'+$(this).data("id")+'"';
			}
		});
		Ids+="]";
		ajaxPath = ajaxPath+"&selectingAll=true";
	} else {
		var Ids="[";
		$(".listItemHighlight",$scope).each(function(){
			if (Ids=="[") {
			Ids+='"'+$(this).data("id")+'"';
			} else {
			Ids+=',"'+$(this).data("id")+'"';
			}
		});
		Ids+="]";
	}
	ajaxPath = ajaxPath+"&ids="+Ids;
