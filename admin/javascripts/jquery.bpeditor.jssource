	for (var i=0; i < zones.length; i++) {
		if (zones[i]!="") {
		if (typeof LangVars["Custom_"+zones[i]]!='undefined') {
			var friendlyname = LangVars["Custom_"+zones[i]];
		} else {
			var friendlyname = zones[i].replace(/_/g," ");
