			$("#bpe_area").empty().append($("#undoStack .viewing").prev().data("content"));
			$("#undoStack .viewing").prev().addClass("viewing");
			$("#undoStack .viewing:last").removeClass("viewing");
		} else {
			$("#finalRedo").data("content",$("#entryTextAreaVal").val());
