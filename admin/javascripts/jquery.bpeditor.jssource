			} else {
				$("#iframe"+livepreviewToggle)[0].contentWindow.updateCMSText(toHTML(htmlentities($this.val()).replace(/(?:\r\n|\r|\n)/g, '<br />')));				
			}
		}
		var el = $wrap.attr("data-el");		
