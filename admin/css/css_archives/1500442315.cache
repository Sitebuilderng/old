/* Video Player */

#bothPanes {
	background:#fff;
	width:100%;
	min-height: 100vh;
	position:relative;
	transition: all 1s;
	-webkit-transition: all 1s; /* Safari */
	transform-origin: 50% 50%;
	-webkit-transform-origin: 50% 50%;
	opacity:1;
}
.playingHelp #bothPanes {
	transform:scale(0.9);
	-webkit-transform:scale(0.9);
	opacity:0.4;
}
#videoPlayer {
	transform:translateY(120%);
	-webkit-transform:translateY(120%);
	-webkit-backface-visibility: hidden;
	position:fixed;
	top:0px;
	left:0px;
	bottom:0px;
	right:0px;
	z-index:999999;
	transition: max-height 1s 0s, transform 1s;
	-webkit-transition: max-height 1s 0s, -webkit-transform 1s; 
}
#videoPlayerInner {
	background:rgba(255,255,255,0.95);
	border-radius:8px;
	overflow:hidden;
	position:absolute;
	top:50%;
	left:50%;
	transform:translateY(-50%) translateX(-50%);
	-webkit-transform:translateY(-50%) translateX(-50%);
	box-shadow:4px 4px 10px rgba(0,0,0,0.5);
}
.playingHelp #videoPlayer {
	transition-delay:0s;
	transform:translateY(0%);
	-webkit-transform:translateY(0%);
}

