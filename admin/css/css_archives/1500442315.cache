  border-radius: 100%;  
  -webkit-animation: sk-scaleoutb 1.0s infinite ease-in-out;
  animation: sk-scaleoutb 1.0s infinite ease-in-out;
}

@-webkit-keyframes sk-scaleoutb {
  0% { -webkit-transform: scale(0) }
  100% {
    -webkit-transform: scale(1.0);
    opacity: 0;
  }
}

@keyframes sk-scaleoutb {
  0% { 
    -webkit-transform: scale(0);
    transform: scale(0);
  } 100% {
    -webkit-transform: scale(1.0);
    transform: scale(1.0);
    opacity: 0;
  }
}
