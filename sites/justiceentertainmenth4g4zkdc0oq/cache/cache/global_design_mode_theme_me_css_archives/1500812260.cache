.panzoom .backstretch img
{
	opacity:1;
	transition:1s opacity;
}
.panzoom .backstretch img.hidden {
	opacity:0;
	transition:1s opacity;
}
.panzoom .backstretch .img0 {
 animation : 6s ease 1s normal none infinite panzoom1	;
 transform-origin:20% 80%;
}
.panzoom .backstretch .img1 {
 animation : 6s ease 1s normal none infinite panzoom2;
  transform-origin:5% 80%;
}

.panzoom .backstretch .img2 {
 animation : 6s ease 1s normal none infinite panzoom3	;
  transform-origin:90% 20%;
}

.panzoom .backstretch .img3 {
 animation : 6s ease 1s normal none infinite panzoom4	;
  transform-origin:80% 80%;
}

.panzoom .backstretch .img4 {
 animation : 6s ease 1s normal none infinite panzoom5	;
  transform-origin:20% 20%;
}

@keyframes panzoom1{
 0%{
      transform:scale(1);
   }
 100%{

      transform:scale(1.2) translateX(2%) translateY(2%);
   }
}
@keyframes panzoom2{
 0%{
        transform:scale(1);
   }
 100%{

      transform:scale(1.2) translateX(-1%) translateY(-1%);
   }
}
@keyframes panzoom3{
 0%{
      transform:scale(1);
   }
 100%{

      transform:scale(1.2) translateX(-2%) translateY(1%);
   }
}
@keyframes panzoom4{
 0%{
      transform:scale(1);
   }
 100%{

      transform:scale(1.2) translateX(2%) translateY(1%);
   }
}
@keyframes panzoom5{
 0%{
      transform:scale(1);
   }
 100%{

      transform:scale(1.2) translateX(-1%) translateY(2%);
   }
}
