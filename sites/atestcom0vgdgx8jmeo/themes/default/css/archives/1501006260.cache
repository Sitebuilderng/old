/* Columns */
.column_row:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}
.column_row {
	margin:20px 0;
	width:100%;
}
.column_row:first-child {
  margin-top: 0;
}
.column_row:last-child {
  margin-bottom: 0;
}
.column > *:first-child {
	margin-top:0;
}
.column {
	float:left;
	margin:0 2%;
}
