			if (percent!=0) {
				percent = percent*100;				
			}
			$(".jp-play-bar",$(this).parent()).css("width",percent+"%");
			if ($(this).parents(".videoPlayerWrapper").length) {
				var playerid = $(".jplayer",$(this).parents(".videoPlayerWrapper")).attr("id");
			}
			if ($(this).parents(".videoPlayerWrapper").length) {
