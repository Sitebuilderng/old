    {
        margin:0;
        padding:0;
        position: relative;
        z-index: 1;
        margin:0;
        text-align: right;
    }
	    #expandNav > ul > li
	    {

	        position: relative;
	        z-index: 1;
	        display: inline-block;
	        *display: inline; /* IE Fix */
			vertical-align:top;
			text-transform:uppercase;
	    }
	    #searchFormSmall {
	        display: inline-block;
	        *display: inline; /* IE Fix */
	        padding: 0 0 0 20px;
	        margin:0px 0 0px;
	    }
        #searchFormSmall input {
            width: 120px;
			background:transparent;
			color:#fff;
			
        }
            #expandNav > ul:hover {
                z-index: 2;
            }

            #expandNav > ul > li > a
            {
                height: 100%;
                padding:7px 20px;
				font-size:14px;
				font-weight:700;
            }
            #expandNav.size_1 > ul > li > a {
                padding:7px 5px;   
            }
			#expandNav.size_1 > ul.noSearch {
				margin-right:-5px;
			}
            #expandNav.size_2 > ul > li > a {
                padding:7px 10px; 
            }
			#expandNav.size_2 > ul.noSearch {
				margin-right:-10px;
			}
            #expandNav.size_3 > ul > li > a {
                padding:7px 20px;   
            }
			#expandNav.size_3 > ul.noSearch {
				margin-right:-20px;
			}
			
            @media only screen and (max-width: 1000px) {
                #expandNav > ul > li > a {
                    padding:7px 10px;
